{
	"name": "ts-graphql-api-server-sample",
	"version": "0.1.0",
	"description": "A really basic GraphQL Api Server for study proposers.",
	"keywords": [
		"graphql",
		"api",
		"server"
	],
	"scripts": {
		"ts:check": "tsc --noEmit",
		"ts:check-watch": "npm run ts:check -- --watch",
		"ts:build": "tsc --emitDeclarationOnly",
		"ts:watch": "tsc --watch",
		"js:watch": "nodemon -q build/index.js",
		"dev": "concurrently \"npm:ts:watch\" \"npm:js:watch\"",
		"lint": "eslint --ext .ts,.js --ignore-path .gitignore --fix src",
		"format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
		"prepare": "npm run ts:build",
		"version": "npm run format && git add -A src",
		"postversion": "git push && git push --tags",
		"prepublishOnly": "npm test && npm run lint",
		"preversion": "npm run lint",
		"test": "jest",
		"test:debug": "node --inspect node_modules/.bin/jest --runInBand"
	},
	"main": "index.js",
	"private": true,
	"author": "caiquearaujo",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/caiquearaujo/ts-graphql-api-server-sample.git"
	},
	"bugs": {
		"url": "https://github.com/caiquearaujo/ts-graphql-api-server-sample/issues"
	},
	"dependencies": {
		"@graphql-tools/schema": "^8.3.14",
		"apollo-server-express": "^3.8.2",
		"debug": "^4.3.4",
		"dotenv": "^16.0.1",
		"express": "^4.18.1",
		"express-rate-limit": "^6.4.0",
		"graphql": "^16.5.0",
		"graphql-depth-limit": "^1.1.0",
		"graphql-query-complexity": "^0.11.0",
		"helmet": "^5.1.0",
		"http-errors": "^2.0.0",
		"module-alias": "^2.2.2",
		"morgan": "^1.10.0"
	},
	"devDependencies": {
		"@types/express": "^4.17.13",
		"@types/express-rate-limit": "^6.0.0",
		"@types/graphql-depth-limit": "^1.1.3",
		"@types/http-errors": "^1.8.2",
		"@types/jest": "^28.1.1",
		"@types/morgan": "^1.9.3",
		"@types/node": "^17.0.43",
		"@types/supertest": "^2.0.12",
		"@typescript-eslint/eslint-plugin": "^5.28.0",
		"@typescript-eslint/parser": "^5.28.0",
		"@zerollup/ts-transform-paths": "^1.7.18",
		"concurrently": "^7.2.2",
		"eslint": "^8.17.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.0.0",
		"jest": "^28.1.1",
		"nodemon": "^2.0.16",
		"prettier": "^2.7.0",
		"supertest": "^6.2.3",
		"ts-jest": "^28.0.5",
		"typescript": "^4.7.3"
	},
	"_moduleAliases": {
		"@": "build/"
	}
}
